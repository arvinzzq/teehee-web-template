{
  "name": "teehee-web-template",
  "version": "0.0.1",
  "description": "teehee web template",
  "main": "index.js",
  "scripts": {
    "start:dev": "NODE_ENV=development npm run start:dev:server",
    "start:dev:server": "nodemon --exec 'teehee start' -w server -e 'js json' server/app.js | bunyan --time local -o short",
    "webpack:dev": "webpack --config webpack.dev.js --mode development --watch",
    "start:prod:server": "start production mode server side",
    "webpack:prod": "webpack --config webpack.prod.js --mode production",
    "publish:client": "publish client resource to cdn",
    "publish:server": "compile server side code"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "ip": "^1.1.5",
    "koa": "^2.5.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "nodemon": "^1.17.5",
    "zeass": "0.0.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "browser-sync": "^2.24.6",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "sass-loader": "^7.0.3",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.3"
  },
  "config": {
    "baport": 8000,
    "development": {
      "host": "127.0.0.1",
      "port": "8888"
    },
    "production": {
      "host": "zonzz.com",
      "port": "8888"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zz1211/teehee-web-template.git"
  },
  "keywords": [
    "teehee",
